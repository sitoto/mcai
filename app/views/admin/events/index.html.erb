<h1>Listing admin_events</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Status</th>
    <th>Created at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_events.each do |admin_event| %>
  <tr>
    <td><%= admin_event.name %></td>
    <td><%= admin_event.status %></td>
    <td><%= admin_event.created_at %></td>
    <td><%= link_to 'Show', admin_event %></td>
    <td><%= link_to 'Edit', edit_admin_event_path(admin_event) %></td>
    <td><%= link_to 'Destroy', admin_event, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_admin_event_path %>
