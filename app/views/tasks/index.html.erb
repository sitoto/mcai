<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Preview</th>
    <th>Name</th>
    <th>Web url</th>
    <th>Verification</th>
    <th>Status</th>
    <th>Exec num</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= link_to 'Test', preview_task_path(task) %></td>
    <td><%= task.name %></td>
    <td><%= task.web_url %></td>
    <td><%= task.verification %></td>
    <td><%= task.status %></td>
    <td><%= task.exec_num %></td>
    <td><%= link_to 'Show', task %></td>
    <td><%= link_to 'Edit', edit_task_path(task) %></td>
    <td><%= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %></td>

  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Task', new_task_path %>
