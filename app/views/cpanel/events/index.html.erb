<h1>事件列表</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Status</th>
    <th>IP</th>
    <th>Created at</th>
    <th>Note</th>
  </tr>

  <% @cpanel_events.each do |cpanel_event| %>
    <tr>
      <td><%= cpanel_event.name %></td>
      <td><%= cpanel_event.status %></td>
      <td><%= cpanel_event.from_ip %></td>
      <td><%= cpanel_event.created_at %></td>
      <td><%= cpanel_event.note %></td>
    </tr>
  <% end %>
</table>
<%= paginate @cpanel_events %>
